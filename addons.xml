<addon id="plugin.video.IPCableCloud" name="LIVE TV" version="1.3" provider-name="IPCC">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="init.py">
        <provides>video executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <disclaimer lang="en">You may use and share this addon however you like, you may NOT edit this addon in any way without conesnt of developer.</disclaimer>
