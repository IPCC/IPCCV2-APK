<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="script.IPCableCloud" name="IP Cable Cloud" version="1.3.0" provider-name="IPCC">
	<requires>
		<import addon="xbmc.python" version="2.19.0"/>
		<import addon="script.module.requests"/>
	</requires>
	<extension point="xbmc.python.script" library="addon.py">
            <provides>executable</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="login"/>
	<extension point="xbmc.addon.metadata">
		<language></language>
		<platform>all</platform>
		<license>GPLv2</license>
	</extension>
</addon>

<addon id="script.IPCableCloudGuide" name="IP Cable Cloud Guide" version="1.3.0" provider-name="IPCC">
	<requires>
		<import addon="xbmc.python" version="2.19.0"/>
		<import addon="script.module.simplejson" version="3.3.0"/>
		<import addon="script.module.requests"/>
		<import addon="script.module.beautifulsoup4"/>
	</requires>
	<extension point="xbmc.python.script" library="addon.py">
            <provides>executable</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="login"/>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">This Guide allows you to combine some of your favourite live TV plugins for use with a fully working EPG</summary>
		<language></language>
		<platform>all</platform>
		<license>GPLv2</license>
	</extension>
</addon>

